<header>
  <span class="title"><h1>My Locations</h1></span>
</header>

<table class="table table-hover">
  <thead><tr>
   <th>Nickname </th> 
   <th>Address </th> 
   <th>Monthly Rate </th> 
   <th>Interest Rate </th> 
   <th>Pets </th> 
   <th>Capacity </th> 
 </tr></thead> 
 <tbody>
   <% @location.each do |location| %>
   <tr>
     <td><%=location.nickname%> </td> 
     <td><%=location.address%> </td> 
     <td><%=location.rate%> </td> 
     <td><%=location.interest_rate%> </td> 
     <td><%=location.allow_pets?%> </td> 
     <td><%=location.no_people%> </td> 
   </tr> 
 </tbody> 	
 <% end %>
</table>